<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jsp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">

	<welcome-file-list>
		<welcome-file>welcome.html</welcome-file>
	</welcome-file-list>

	
	<servlet>
		<servlet-name>greeter</servlet-name>
		<servlet-class>basic.web.app.GreetingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>greeter</servlet-name>
		<url-pattern>/welcome.gws</url-pattern>
	</servlet-mapping>


</web-app>

